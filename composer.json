{
    "name": "interfrog/if_whitevita",
    "type": "typo3-cms-extension",
    "version": "1.0.0",
    "description": "IfPage Whitevita - Distribution package based upon the if_themeconfiguration extension with basic blog template and styles",
    "keywords": [
        "TYPO3",
        "extension",
        "ifPage",
        "Theme",
        "WhiteVita"
    ],
    "homepage": "http://interfrog.de",
    "authors": [{
        "name": "Interfrog Produktion GmbH",
        "role": "Developer",
        "homepage": "http://interfrog.de"
    }],
    "license": [
        "GPL-2.0+"
    ],
    "require": {
        "typo3/cms-core": ">=7.6.3,<8.0"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "autoload": {
        "psr-4": {
            "Interfrog\\IfWhitevita\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        }
    }
}
